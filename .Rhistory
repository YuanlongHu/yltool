convCorrMatrix <- function(mat, pmat) {
ut <- upper.tri(mat)
data.frame(
row = rownames(mat)[row(mat)[ut]],
column = rownames(mat)[col(mat)[ut]],
cor  =(mat)[ut],
p = pmat[ut]
)
}
